			dir3 = ROBOT_BACKWARD;
			break;
		case ROBOT_BACKWARD:
			dir3 = ROBOT_BACKWARD;
			break;
		default:
			break;
	}

	setJointMovementStateNB(0, dir1);
	setJointMovementStateNB(1, dir2);
	setJointMovementStateNB(2, dir3);

	// success
	return 0;
}

